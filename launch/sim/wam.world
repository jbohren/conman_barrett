<?xml version ='1.0'?>
<sdf version ='1.4'>
  <world name='default'>
    <plugin name="initial_time" filename="libgazebo_initial_time.so">
      <wall_time/>
    </plugin>
    <scene>
      <background>0 0 0 0</background>
    </scene>
    <include>
      <uri>model://ground_plane</uri>
      <pose>0 0 -1.0 0 0 0</pose>
    </include>
    <!--<include>-->
      <!--<uri>model://sun</uri>-->
      <!--</include>-->

    <!-- key light (sun) -->
    <light type="directional" name="sun">
      <cast_shadows>true</cast_shadows>

      <pose>0 0 10 0 0 0</pose>
      <diffuse>0.4 0.4 0.4 1</diffuse>
      <specular>0.9 0.9 0.9 1</specular>

      <attenuation>
        <range>1000</range>
        <constant>0.9</constant>
        <linear>0.01</linear>
        <quadratic>0.001</quadratic>
      </attenuation>

      <direction>0.0 0 -1.0</direction>
    </light>

    <!-- fill light (warm) -->
    <light type="point" name="fill">
      <cast_shadows>false</cast_shadows>

      <pose>0.3 2.75 1 0 0 0</pose>
      <diffuse>0.11 0.08 0.04 1</diffuse>
      <specular>0.9 0.9 0.9 1</specular>

      <attenuation>
        <range>20.0</range>
        <constant>0.5</constant>
        <linear>0.01</linear>
        <quadratic>0.001</quadratic>
      </attenuation>
    </light>

    <!-- back light / kicker (white) -->
    <light type="point" name="kicker">
      <cast_shadows>false</cast_shadows>

      <pose>0.81 0.88 -0.5 0 0 0</pose>
      <diffuse>0.4 0.4 0.4 1</diffuse>
      <specular>0.9 0.9 0.9 1</specular>

      <attenuation>
        <range>20.0</range>
        <constant>0.5</constant>
        <linear>0.01</linear>
        <quadratic>0.001</quadratic>
      </attenuation>
    </light>

    <physics type="ode">
      <gravity>0 0 -9.81</gravity>
      <max_step_size>0.001</max_step_size>
      <real_time_update_rate>1000.0</real_time_update_rate>
      <!-- real_time_factor is completely ignored by gazebo -->
      <!--<real_time_factor>0.01</real_time_factor>-->
      <ode>
        <solver>
          <type>quick</type>
          <use_dynamic_moi_rescaling>false</use_dynamic_moi_rescaling>
          <!--<min_step_size>0.001</min_step_size>-->
          <iters>50</iters>
          <sor>1.4</sor>
        </solver>
        <constraints>
          <cfm>0.0</cfm>
          <erp>0.2</erp>
          <contact_max_correcting_vel>100.0</contact_max_correcting_vel>
          <contact_surface_layer>0.0</contact_surface_layer>
        </constraints>
      </ode>
    </physics>
  </world>
</sdf>
